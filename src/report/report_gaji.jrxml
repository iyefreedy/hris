	<queryString>
		<![CDATA[SELECT *,
(SELECT SUM(t_gaji.nominal) FROM t_gaji ) as total,
YEAR(t_gaji.created_at) AS tahun,
MONTHNAME(t_gaji.created_at) AS bulan
FROM t_pegawai JOIN t_gaji
WHERE t_gaji.bulan=$P{bulan}
AND YEAR(t_gaji.created_at)=$P{tahun}
AND t_pegawai.id=t_gaji.pegawai_id]]>
	</queryString>
