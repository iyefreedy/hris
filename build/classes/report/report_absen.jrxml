	<queryString>
		<![CDATA[SELECT *,
DATE_FORMAT(t_absen.tanggal, '%W', 'id_ID') AS hari,
ROW_NUMBER() OVER(ORDER BY t_absen.tanggal) AS row_num, t_departemen.nama AS dept_nama,
MONTHNAME(t_absen.tanggal) AS bulan,
YEAR(t_absen.tanggal) AS tahun
FROM t_absen JOIN t_pegawai JOIN t_departemen
WHERE t_absen.pegawai_id=t_pegawai.id
AND t_pegawai.dept_id=t_departemen.id
AND t_pegawai.id=$P!{pegawai_id}
AND MONTHNAME(t_absen.tanggal)=$P!{bulan}
AND YEAR(t_absen.tanggal)=$P{tahun}]]>
	</queryString>
